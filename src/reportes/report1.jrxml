	<queryString language="SQL">
		<![CDATA[SELECT
     practicas.`Titulo` AS practicas_Titulo,
     practicas.`fecha_inicio` AS practicas_fecha_inicio,
     practicas.`fecha_fin` AS practicas_fecha_fin,
     practicas.`tema` AS practicas_tema,
     practicas.`descripcion` AS practicas_descripcion,
     practicas.`conclusiones` AS practicas_conclusiones,
     practicas.`acuerdos` AS practicas_acuerdos,
     metodologias.`descripcion` AS metodologias_descripcion,
     localidades.`nombre` AS localidades_nombre,
     circunscripciones.`descripcion` AS circunscripciones_descripcion
FROM
     `metodologias` metodologias INNER JOIN `practicas` practicas ON metodologias.`id` = practicas.`metodologia`
     INNER JOIN `localidades` localidades ON practicas.`localidades` = localidades.`id`
     INNER JOIN `circunscripciones` circunscripciones ON localidades.`circunscripciones` = circunscripciones.`id`]]>
	</queryString>
